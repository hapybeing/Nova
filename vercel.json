{
  "rewrites": [
    {
      "source": "/proxy/api/:path*",
      "destination": "https://api.mangadex.org/:path*"
    },
    {
      "source": "/proxy/uploads/:path*",
      "destination": "https://uploads.mangadex.org/:path*"
    },
    {
      "source": "/proxy/comick/:path*",
      "destination": "https://api.comick.io/:path*"
    }
  ]
}
